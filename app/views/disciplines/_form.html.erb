<%= simple_form_for(@discipline) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <div class="row">
      <div class="col s12 l6 separator padding">
        <%= f.input :name %>
        <%= f.input :duree %>
        <%= f.collection_select(:frequence_id, Frequence.all, :id, :name, {:class=>'visible'}) %>
      </div>
      <div class="col s12 l6  padding">
        <%= f.input :tarif1 %>
        <%= f.input :tarif2 %>
        <%= f.input :tarif3 %>
      </div>
    </div>
    <div class="form-actions center">
      <%= f.button :submit %>
    </div>
  </div>
<% end %>
