    <div class="myflex content-title">
      <p><%= render 'shared/logo' %></p>
      <%= render 'shared/logout' %>
    </div>

    <div class="content-bandeau myflex">
      <div>
        Connecté comme : <%= @adherent.function.name %>
          <%= link_to adherent_profile_path(@adherent) do %>
            <i class="fa fa-user"></i>
          <% end %>
      </div>
      <%= link_to destroy_user_session_path, method: :delete do %>
        <i class="fa fa-sign-out"></i>
      <% end %>
    </div>

      <div class="row">
        <div class="col s12 m6 l3">
          <%= render 'profilecol' %>
        </div>

        <div class="col s12 m6 l3">
          <ul>
            <li class="collection-item"><%= link_to " Bureau", bureau_path, class: "fa fa-users" %></li>
            <li class="collection-item"><%= link_to " Professeurs", profs_path, class: "fa fa-user-secret" %></li>
            <li class="collection-item"><%= link_to " Adhérents", adherent_index_path, class: "fa fa-address-book" %></li>
            <li class="collection-item"><%= link_to " Elèves", students_path, class: "fa fa-graduation-cap" %></li>
          </ul>
        </div>
        <div class="col s12 m6 l3">
          <ul>
            <li class="collection-item"><%= link_to " Créneaux", adherent_crenaus_path(@adherent), class: "fa fa-calendar" %></li>
            <li class="collection-item"><%= link_to " Inscriptions", "#", class: "fa fa-info" %></li>
          </ul>
        </div>
        <div class="col s12 m6 l3">
          <ul>
            <li class="collection-item"><%= link_to " Instruments", instruments_path, class: "fa fa-music" %></li>
            <li class="collection-item"><%= link_to " Formules", disciplines_path, class: "fa fa-server" %></li>
            <li class="collection-item"><%= link_to " Tarifs", tarifs_path, class: "fa fa-bitcoin" %></li>
