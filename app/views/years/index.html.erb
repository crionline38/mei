<div class="title">
  <h4><a href="#" onClick="history.go(-1);return true;">
        <i class="fa fa-chevron-circle-left"></i></a></h4>
  <h4>Saisons</h4>
  <h4><%= link_to new_year_path do  %>
  <i class="fa fa-plus-circle" aria-hidden="true" title="Ajouter une saison"></i>
  <% end %></h4>
</div>

<div class="mycard">
  <table class="striped responsive-table">
    <thead>
      <tr>
        <th>Name</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @years.each do |year| %>
        <tr>
          <td><%= year.name %></td>
          <td><%= link_to 'Renomer', edit_year_path(year) %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

</div>
